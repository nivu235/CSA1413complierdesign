#include<stdio.h>
#include<string.h>
int main()
{	int i,n,fact=0;
	char a[100];
	printf("enter the sentence");
	scanf("%s",&a);
	n=strlen(a);
	for(i=0;i<n;i++)
	{
		if(a[0]=='/')
		{
			fact=1;
		}
		else
			{
				fact=0;
			}
			
		
	}
	if(fact==1)
	{
		printf("it is a commend");
	}
	else
	{
		printf("it is not a command");
	}
}
